			</panelchildren>
		</panel>
		<panel title="错误" collapsible="true">
			<panelchildren>
				<listbox id="errorlist" fixedLayout="true">
					<listhead>
						<listheader width="20%" label="时间"
							sort="none">
						</listheader>
						<listheader width="40%" label="名称"
							sort="none">
						</listheader>
						<listheader width="40%" label="描述"
							sort="none">
						</listheader>
					</listhead>
				</listbox>
			</panelchildren>
		</panel>
		<panel title="危险" collapsible="true">
